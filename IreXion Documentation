Library:AddGui(properties)
	<table> properties -> {
		<table> Title -> {
			<string> Title1,
			<string> Title2
		}
		<Color3> ThemeColor
		<Enum.KeyCode> ToggleKey
	}

	return -> <table> Gui

Gui:Toggle(bool)
	<bool> bool

Gui:AddTab(name)
	<string> name

	return -> <table> Tab

Tab:AddCategory(name)
	<string> name

	return -> <table> Category

Category:AddButton(name, func)
	<string> name
	<function> func

	return -> <table> Button

Category:AddToggle(name, default, func)
	<string> name
	<bool> default
	<function> func

	return -> <table> Toggle

Toggle:Set(bool)
	<bool> bool

Category:AddBox(name, func)
	<string> name
	<function> func

	return -> <table> Box

Category:AddLabel(name)
	<string> name

	return -> <table> Label

Category:AddDualLabel(properties)
	<table> properties -> {
		<string> Text1,
		<string> Text2
	}

	return -> <table> DualLabel

Category:AddSlider(name, min, max, default, func)
	<string> name
	<int> min
	<int> max
	<int> default
	<function> function

	return -> <table> Slider

Slider:Set(int)
	<int> int

Category:AddDropdown(name, items, func)
	<string> name
	<table<string>> items -> table of items
	<function> func

	return -> <table> Dropdown

Dropdown:AddItem(name)
	<string> name
	
	return -> <instance> TextButton

Dropdown:RemoveItem(name)
	<string> name

Category:AddBind(name, bind, func)
	<string> name
	<Enum.KeyCode> bind
	<function> func

	return -> <table> Bind

Bind:Set(bind)
	<Enum.KeyCode> bind

return -> <table> Library
